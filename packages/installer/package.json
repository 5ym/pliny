  },
  "author": "Timothy Lin <timothy0336@hotmail.com> (https://timlrx.com)",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "pliny",
    "installer"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timlrx/pliny.git"
  },
  "dependencies": {
    "@babel/core": "7.18.0",
    "@babel/plugin-transform-typescript": "7.18.1",
    "@types/jscodeshift": "0.11.5",
    "ast-types": "0.14.2",
    "console-table-printer": "^2.7.5",
    "cross-spawn": "7.0.3",
    "diff": "5.0.0",
